#include<bits/stdc++.h>
using namespace std;

int main()
{
   #ifndef ONLINE_JUDGE
   freopen("input.txt","r",stdin); 
    freopen("q.txt","w",stdout); 
    #endif

    int arr[]{12,2,13,434,65};
    int n=5;
   int maxx=INT_MIN,minn=INT_MAX;
   for(int i=0;i<n;i++)
   {
   	maxx=max(maxx,arr[i]);
    minn=min(minn,arr[i]);
   }
   cout<<"max = "<<maxx<<" min = "<<minn;
   
}
